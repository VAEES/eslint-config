{
    "name": "@b2rise/eslint-config",
    "version": "0.0.1",
    "author": "Ot√°vio Baziewicz Filho",
    "license": "MIT",
    "description": "A standardized set of eslint shareable configs",
    "homepage": "https://github.com/VAEES/eslint-config#readme",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/VAEES/eslint-config.git"
    },
    "bugs": {
        "url": "https://github.com/VAEES/eslint-config/issues"
    },
    "keywords": [
        "eslint",
        "eslintconfig"
    ],
    "os": [
        "win32",
        "darwin",
        "linux"
    ],
    "cpu": [
        "x64"
    ],
    "main": "dist/bundle.js",
    "module": "dist/bundle.mjs",
    "types": "dist/src/index.d.ts",
    "type": "module",
    "exports": {
        ".": {
            "require": "./dist/bundle.js",
            "import": "./dist/bundle.mjs",
            "types": "./dist/src/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "prepare": "yarn build",
        "typecheck": "tsc --noEmit",
        "lint": "eslint .",
        "test": "echo \"Skipping...\"",
        "format": "prettier --write .",
        "format:check": "prettier --check .",
        "build": "rollup -c",
        "dev": "rollup -c -w"
    },
    "dependencies": {
        "@eslint/js": "^9.28.0",
        "@types/eslint-config-prettier": "^6.11.3",
        "@typescript-eslint/utils": "^8.33.1",
        "eslint": "^9.28.0",
        "eslint-config-prettier": "^10.1.5",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "typescript-eslint": "^8.33.1"
    },
    "devDependencies": {
        "@rollup/plugin-terser": "^0.4.4",
        "@rollup/plugin-typescript": "^12.1.2",
        "@types/node": "^22",
        "prettier": "^3.5.3",
        "rollup": "^4.41.1",
        "tslib": "^2.8.1",
        "typescript": "^5.8.3"
    }
}